{
	"name": "dugan/zbw",
	"description": "Boston ARTCC website & application",
	"license": "Proprietary",
	"type": "application",
	"require": {
		"laravel/framework": "5.0.*",
		"illuminate/html": "~5.0.0",
        "fkubis/teamspeak-php-framework": "dev-master",
        "curl/curl": "dev-master",
        "pda/pheanstalk": "~3.0.1",
        "cartalyst/sentry": "dev-feature/laravel-5",
        "bugsnag/bugsnag-laravel": "~1.4.0",
        "doctrine/dbal": "2.3.*",
        "robclancy/presenter": "1.*",
        "laracasts/commander": "~1.4.3",
        "mjohnson/decoda": "~6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
        "barryvdh/laravel-ide-helper": "~2.0.3",
        "barryvdh/laravel-debugbar": "~1.8.0",
        "fzaninotto/faker": "~1.4.0",
        "codeception/codeception" : "~2.0.11",
        "mockery/mockery": "~0.9"

	},
	"autoload": {
		"classmap": [
			"database",
            "app/Models"
		],
		"psr-4": {
			"Zbw\\": "app/",
            "Tests\\": "tests/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
